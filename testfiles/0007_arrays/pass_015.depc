func xs_or_ys(bool_t which, u64_t n, array_t(i32_t, n) xs, array_t(i32_t, n) ys) -> array_t(i32_t, n)
{
    if (which)
        return xs;
    else
        return ys;
}

func xs_or_ys_2(bool_t which, array_t(i32_t, 2) xs, array_t(i32_t, 2) ys) -> array_t(i32_t, 2)
{
    return xs_or_ys(which, 2, xs, ys);
}
