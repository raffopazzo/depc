func f1(u64_t n, array_t(i32_t, n) because true xs, 0 true_t(0 < n)) -> i32_t
{
    return xs[0];
}

func f2(u64_t n, array_t(array_t(i32_t, n) because true, n) xs, 0 true_t(1 < n)) -> i32_t
{
    return xs[1][1];
}
