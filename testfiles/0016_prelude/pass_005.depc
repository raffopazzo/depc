func one(bool_t a, 0 true_t(a)) -> i32_t { return 1; }
func zero(bool_t a, 0 true_t(not a)) -> i32_t { return 0; }

func f1(bool_t a) -> i32_t
{
    if (a == true)
        return one(a, auto);
    else
        return zero(a, auto);
}

func f2(bool_t a) -> i32_t
{
    if (a == false)
        return zero(a, auto);
    else
        return one(a, auto);
}
