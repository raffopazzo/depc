func f1((i32_t, u32_t) x) -> u32_t
{
    return x[1];
}
func f2((i32_t,) x) -> i32_t
{
    return x[0];
}
func f3((i32_t x, u32_t) x) -> i32_t
{
    return x[0];
}
func f4(array_t((i32_t, u32_t), 3) xs) -> u32_t
{
    return f1(xs[0]);
}
