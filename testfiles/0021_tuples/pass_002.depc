func f(array_t((i32_t, i16_t), 2) xs, u64_t i, 0 true_t(i < 2)) ->  (i32_t, i16_t)
{
    return xs[i];
}
func f2(((i32_t, i16_t), (i32_t, i16_t)) xs, bool_t which) ->  (i32_t, i16_t)
{
    if (which)
        return xs[0];
    else
        return xs[1];
}
func g((i32_t, i16_t) x, (i32_t, i16_t) y, bool_t which) -> (i32_t, i16_t)
{
    if (which)
        return x;
    else
        return y;
}
