struct t
{
    i32_t x;
    i32_t y;
    array_t(i32_t, 3) v;
    array_t(i32_t, 3) w;
};

func f0(0 scope_t a, ref_t(i32_t, a) p, ref_t(i32_t, a) q) -> i32_t
{
    if (*p == 0)
        return *q;
    else
        return *p;
}

func f1(array_t(i32_t, 2) v) -> i32_t { return f0(scopeof(v), &v[0], &v[1]); }
func f2((i32_t, i32_t) v) -> i32_t { return f0(scopeof(v), &v[0], &v[1]); }
